{
  "manifest_version": 3,
  "name": "OpenTalkGPT",
  "description": "This extension hosts an ollama ui web server on localhost",
  "version": "1.1",
  "action": {},
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "http://localhost:*/*",
    "https://localhost:*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  }
}